{
  "name": "vanilla-template",
  "private": true,
  "version": "0.0.1",
  "description": "Javascript code snippet template",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --port 8080",
    "build": "parcel build index.html --public-url '.'",
    "jasmine": "node spec/support/jasmine.config.js",
    "serve": "http-server ./dist -c-1 -p 8080",
    "test": "npm run build && start-server-and-test 'npm run serve' 8080 'npm run jasmine'",
    "test:ci": "start-server-and-test 'npm run serve' 8080 'npm run jasmine'"
  },
  "dependencies": {
    "handsontable": "latest"
  },
  "devDependencies": {
    "http-server": "^0.12.3",
    "jasmine": "^3.6.4",
    "jasmine-console-reporter": "^3.1.0",
    "parcel-bundler": "^1.6.1",
    "puppeteer": "^7.0.1",
    "start-server-and-test": "^1.12.0"
  }
}
